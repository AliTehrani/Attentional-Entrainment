#!/bin/bash

for toneLength in {1..3}; do
    ioiMin=$((toneLength+1))
    ioiMax=$((toneLength*2))
    for ((ioi=ioiMin;ioi<=ioiMax;++ioi)); do
	for ((watchLength=2;watchLength<=ioiMax;++watchLength)); do
	    dir=watch-$watchLength-tone-$toneLength-ioi-$ioi
	    mkdir $dir
	    qsub -v watch=$watchLength, tone=$toneLength, i=$ioi, d=$dir runThis.sh
	done
    done
done
